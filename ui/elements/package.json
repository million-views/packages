{
  "name": "@m5nv/ui-elements",
  "version": "0.9.1",
  "description": "Opinionated, data-driven, themeable UI library",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/million-views/packages.git",
    "directory": "ui/elements"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles.css": {
      "import": "./dist/assets/index.css"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "bugs": {
    "url": "https://github.com/million-views/packages/issues"
  },
  "homepage": "https://github.com/million-views/packages/tree/main/rr-builder",
  "author": {
    "name": "Million Views, LLC.",
    "url": "https://m5nv.com"
  },
  "license": "MIT",
  "contributors": [
    "Hemalatha Manickam (https://github.com/hmanicka)",
    "Vijay Aswadhati (https://github.com/vijay8i)"
  ],
  "scripts": {
    "dev": "vite build --watch --config vite.config.ts",
    "dev:demo": "wrangler dev",
    "build": "npm run build:js && npm run build:types",
    "build:js": "vite build --config vite.config.ts",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist --jsx react --esModuleInterop src/index.tsx",
    "build:demo": "vite build --config demo/vite.demo.config.ts",
    "deploy": "npm run build:demo && wrangler deploy",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.5.0",
    "vite": "^6.3.5",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5"
  }
}